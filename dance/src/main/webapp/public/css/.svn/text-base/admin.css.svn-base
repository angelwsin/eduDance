/* Header */
header {
	color: #fff;
	height: 60px;
	background-color: #2BA8FF;
	padding-top: 10px;
	/* background: url(../image/yc-top.png) no-repeat center; */
}

.site-logo {
	float: left;
	margin-left: 20px;
	width: 275px;
	height: 47px;
	background: url(../image/yc-logo.png) no-repeat top left;
	margin-left: 20px;
}

header ul {
	margin-right: 20px;
}

header li {
	font-size: 12px;
	text-align: right;
	line-height: 30px;
	list-style: none;
}

header a {
	color: #fff;
	padding: 0 5px;
}

header .username {
	color: #ff0;
}
/* Nav */
nav {
	height: 40px;
	border-top: 1px solid #5bbdfa;
	border-bottom: 1px solid #1481c7;
	background: #148adc;
}

nav ul {
	margin-left: 50px;
}

nav li {
	float: left;
	line-height: 40px;
	list-style: none;
}

nav a {
	color: #fff;
	padding: 10px 35px;
}

nav a:hover,nav li.active {
	color: #fff;
	background-image: -webkit-linear-gradient(to left, #0974be, #1f95e7, #0974be);
	background-image: linear-gradient(to left, #0974be, #1f95e7, #0974be);
	background-image: -webkit-linear-gradient(to left, #0974be, #1f95e7, #0974be);
}

/* Section */
.menu {
	float: left;
	width: 13.5%;
	min-height: 720px;
	border-right: 1px solid #e0e0e0;
	background: #fff;
	width: 13.5%;
}

.menu ul li {
	color: #fff;
	line-height: 45px;
	padding-left: 40px;
	list-style: none;
	border-bottom: 1px solid #e0e0e0;
	background: #69c /* 2ea7e0 */;
	list-style: none;
}

.menu dl dt {
	line-height: 40px;
	padding-left: 40px;
	border-bottom: 1px solid #e0e0e0;
}

.menu dt:hover,dt.dt_hover {
	background: #eee;
	color: #00c;
}

.dt_hover a {
	color: #00c;
}

.menu em {
	margin: 0 10px;
}

.ico {
	width: 15px;
	height: 10px;
	padding: 0 2px;
	background: url(../image/ico.png) no-repeat;
	background-position: 0 8px;
}

.ico:hover {
	width: 30px;
	height: 20px;
	padding: 0 2px;
	background: url(../image/ico.png) no-repeat;
	background-position: 0 -8px;
}

.ico-hover {
	width: 30px;
	height: 20px;
	padding: 0 2px;
	background: url(../image/ico.png) no-repeat;
	background-position: 0 -8px;
}

.ico-hover:hover {
	width: 15px;
	height: 10px;
	padding: 0 2px;
	background: url(../image/ico.png) no-repeat;
	background-position: 0 8px;
}

.main {
	float: left;
	width: 86%;
}

.location {
	line-height: 45px;
	padding-left: 30px;
	border-bottom: 1px solid #e0e0e0;
	background: #f7f7f7 url(../image/yc-nav-h2.png) no-repeat 8px;
	overflow: hidden;
}

.content {
	padding: 10px 0 0 15px;
}

/*******************UI [表格]样式***********************/
.ui-table {
	font-family: "Courier New", "宋体";
	font-size: 14px;
	line-height: 1.35em;
	width: 100%;
	margin: 0;
	padding: 0; /*设置cellpadding*/
	text-align: center;
	border: 1px solid #dcdcdc;
	border-collapse: collapse; /*设置cellspacing,去除双倍边框*/
}

/*表格大标题*/
.ui-table caption {
	font-size: 13px;
	font-family: Verdana, ​Arial, ​Helvetica, ​sans-serif;
	color: #000;
	text-align: left;
	border: 1px solid #dcdcdc;
	border-bottom: none;
	margin: 5px auto 0px;
	padding: 4px 0px;
	text-indent: 0.5em;
	white-space: nowrap;
	background: url(../image/table-head-bg.gif);
	text-align: left;
}

/*表格标题列单元格*/
.ui-table th {
	font-family: Microsoft yahei, YaHei, tahoma, arial, "Hiragino Sans GB", \5b8b\4f53;
	padding: 3px 0;
	font-size: 10pt;
	line-height: 2em;
	font-weight: 500;
	/* background: #FFF; */
	border: 1px solid #dcdcdc;
}

/*表格内容单元格*/
.ui-table td {
	font-size: 13px;
	line-height: 2.5em;
	border: 1px solid #dcdcdc;
}

/*表格标题行*/
.ui-table tr.tr-head {
	background: #e9e9e9;
}

/*奇数行*/
.ui-table tr.odd {
	background: #fff;
}

.ui-table tr.odd:hover {
	background: #ffc;
}

/*偶数行*/
.ui-table tr.even {
	background: #f5f5f5;
}

.ui-table tr.even:hover {
	background: #ffc;
}

/*表格操作栏单元格*/
.ui-table .td-action {
	
}

/*表格数据分页状态栏DIV*/
.ui-table-widget {
	width: 100%;
	font-size: 12px;
	line-height: 1.2em;
	font-family: "Courier New", "宋体";
	text-align: right;
	background: #EFEFEF;
}

/*表格数据分页状态栏文字和链接*/
.ui-table-widget span,.ui-table-widget a {
	padding-right: 0.9em;
}

/*表格及表格状态栏中的链接*/
.ui-table-widget a,.ui-table a {
	color: #07519A;
	text-decoration: none;
}

.ui-table-widget a:hover,.ui-table a:hover {
	color: #CC3300;
	text-decoration: underline;
}

/*操作栏链接*/
.td-action a {
	/*继承表格链接样式*/
	
}

/*操作栏按钮*/
.td-action .ui-button {
	/* background: #9CF; */
	padding: 1px 5px;
}

/* .td-action .ui-button:hover {
	background: #CFF;
} */

/*[跳转到] 按钮*/
.ui-table-widget .ui-button {
	border: 0;
	margin: 0 3px;
	padding: 3px 6px;
}

/*表格翻页输入框*/
.ui-table-widget .ui-text {
	margin: auto 2px;
	width: 2.5em;
	height: 0.9em;
}

.ui-table-widget select {
	font-size: 0.9em;
	height: 22px;
	line-height: 22px;
	padding: 0;
	margin: 2px 0;
}

/*表格全局操作按钮DIV*/
.ui-table-action {
	width: 100%;
	padding: 5px;
	text-align: center;
}

/* .ui-table-action .ui-button {
	background: #eee;
} */
.ui-table .td-action {
	width: 60px;
}

.ui-table .th-first {
	font-size: 13px;
	width: 50px;
}

.ui-table .td-sequence {
	display: inline-block;
	text-align: right;
	padding-right: 2px;
	width: 20px;
}

.editor {
	line-height: 20px;
	padding-left: 85px;
}

.ad-table {
	width: 100%;
	line-height: 40px;
	border: 1px solid #dcdcdc;
}

.ad-table td {
	padding-left: 10px;
	line-height: 40px;
	border-right: 1px solid #dcdcdc;
	border-bottom: 1px solid #dcdcdc;
}

.sec-right-form {
	margin: 10px;
	line-height: 35px;
	overflow: hidden;
}

.sec-right-form ul li {
	line-height: 40px;
	list-style: none;
	overflow: hidden;
}

.sec-right-form label {
	display: inline-block;
	width: 150px;
	text-align: right;
}

/* User_Login */
.user-login {
	margin: 0 auto;
	min-height: 900px;
	background: url(../image/loginbg.png) no-repeat center top;
	overflow: hidden;
}

.user-login-wrap {
	margin: 80px auto;
	width: 526px;
	height: 507px;
	background: url(../image/user-login.png) no-repeat center;
	overflow: hidden;
}

.user-login-logo {
	margin: 50px auto;
	text-align: center;
	overflow: hidden;
}

.user-login-logo h3 {
	color: #3d8dcc;
	font-size: 30px;
	font-weight: bold;
}

.user-login-con {
	margin: 0 auto;
	width: 270px;
	overflow: hidden;
}

.user-login-con ul li {
	line-height: 60px;
	list-style-type: none;
}

.user-login-con input {
	float: left;
	border-radius: 35px;
}

.user-login-con label {
	display: none;
}

.user-login-con-span {
	float: left;
	margin-left: 10px;
}

.user-login-con-span {
	line-height: 35px;
}

.login-user,.login-pass {
	padding-left: 40px;
	height: 35px;
	width: 200px;
}

.login-user {
	background: #fff url(../image/login-user.png) no-repeat 5px;
}

.login-pass {
	background: #fff url(../image/login-pass.png) no-repeat 5px;
}

.user-login-login {
	margin: 30px auto;
	width: 277px;
	height: 58px;
	color: #fff;
	font-size: 24px;
	font-weight: bold;
	line-height: 55px;
	text-align: center;
	background: url(../image/user-login-login.png) no-repeat;
	cursor: pointer;
	overflow: hidden;
}