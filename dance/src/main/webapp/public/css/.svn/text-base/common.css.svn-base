.message,.notice {
	border: 1px solid #E6DB55;
	background: none repeat scroll 0 0 #FFFFE0;
	padding: 10px;
	margin: 0 0 5px 0;
	border-radius: 3px;
	line-height: 20px;
}

.message ol,.message ol li {
